{
  "name": "d3-horizontal-bar",
  "version": "1.0.0",
  "description": "D3 With TypeScript make Horizontal Bar",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && rollup -c",
    "serve": "live-server dist --watch=src/**/*.html,dist/**/*.js,dist/**/*.html,src/**/*.css",
    "tsc:w": "tsc -w",
    "rollup:w": "rollup -c -w",
    "start": "npm-run-all --parallel tsc:w rollup:w serve"
  },
  "author": "KIMJUNGHAK",
  "license": "ISC",
  "dependencies": {
    "d3": "^4.13.0"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.57.1",
    "rollup-plugin-fill-html": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "typescript": "^2.7.2"
  }
}
